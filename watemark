
local ScreenGui = Instance.new("ScreenGui")
local Frame = Instance.new("Frame")
local UICorner = Instance.new("UICorner")
local TextLabel = Instance.new("TextLabel")
local epickglasses = Instance.new("ImageLabel")
local UIAspectRatioConstraint = Instance.new("UIAspectRatioConstraint")
local epick2 = Instance.new("ImageLabel")
local UIAspectRatioConstraint_2 = Instance.new("UIAspectRatioConstraint")
local sounds = Instance.new("Sound")

--Properties:
sounds.Parent = ScreenGui
sounds.SoundId = "rbxassetid://6042053626"
ScreenGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
ScreenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

Frame.Parent = ScreenGui
Frame.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
Frame.BorderColor3 = Color3.fromRGB(0, 0, 0)
Frame.BorderSizePixel = 0
Frame.Position = UDim2.new(0.114774644, 0, 0.850715339, 0)
Frame.Size = UDim2.new(0.370215625, 0, 0.149999887, 0)
Frame.Visible = false
Frame.ZIndex = 500

UICorner.Parent = Frame

TextLabel.Parent = Frame
TextLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
TextLabel.BackgroundTransparency = 1.000
TextLabel.BorderColor3 = Color3.fromRGB(0, 0, 0)
TextLabel.BorderSizePixel = 0
TextLabel.Size = UDim2.new(1, 0, 1, 0)
TextLabel.Font = Enum.Font.SourceSans
TextLabel.Text = "Project Epick, made by epick, welcome friend :D"
TextLabel.TextColor3 = Color3.fromRGB(0, 0, 0)
TextLabel.TextScaled = true
TextLabel.TextSize = 14.000
TextLabel.TextWrapped = true

epickglasses.Name = "epick glasses"
epickglasses.Parent = ScreenGui
epickglasses.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
epickglasses.BackgroundTransparency = 1.000
epickglasses.BorderColor3 = Color3.fromRGB(0, 0, 0)
epickglasses.BorderSizePixel = 0
epickglasses.Position = UDim2.new(-0.0135642737, 0, 0.820917547, 0)
epickglasses.Size = UDim2.new(0.138772771, 0, 0.178784028, 0)
epickglasses.ZIndex = 500
epickglasses.Image = "rbxassetid://1447924234"

UIAspectRatioConstraint.Parent = epickglasses

epick2.Name = "epick2"
epick2.Parent = ScreenGui
epick2.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
epick2.BackgroundTransparency = 1.000
epick2.BorderColor3 = Color3.fromRGB(0, 0, 0)
epick2.BorderSizePixel = 0
