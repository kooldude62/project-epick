local function RFIH_fake_script() -- ScreenGui.DASH CIRCLE MAIN 
	local script = Instance.new('LocalScript', ScreenGui)

	local dashedc = script.Parent.ImageLabel.DASH
	
	while wait(1.5) do
		local dc = script.Parent.ImageLabel.DASH:Clone()
		dc.Visible = true
		dc.Parent = script.Parent.ImageLabel
		print("test1")
		local e = script["dashed circle loader."]:Clone()
		e.Parent = dc
		e.Enabled = true
		print("test2")
	end
	
end
coroutine.wrap(RFIH_fake_script)()
-- nil.dashed circle loader. is disabled.
