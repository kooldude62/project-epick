-- Scripts:

local function NSWD_fake_script() -- TextLabel.LocalScript 
	local script = Instance.new('LocalScript', TextLabel)

	
end
coroutine.wrap(NSWD_fake_script)()
local function WKWJE_fake_script() -- TextLabel_2.LocalScript 
	local script = Instance.new('LocalScript', TextLabel_2)

	local textLabel = script.Parent
	
	local speed = 1.3
	
	while true do
		for hue = 0, 1, 0.01 do
			textLabel.TextColor3 = Color3.fromHSV(hue, 1, 1)
			wait(0.03 / speed)
		end
	end
	
	
	
end
coroutine.wrap(WKWJE_fake_script)()
local function XASNBP_fake_script() -- infected.LocalScript 
	local script = Instance.new('LocalScript', infected)

	while task.wait() do
		if game.Workspace:FindFirstChild("stigymaa") and game.Workspace:FindFirstChild("stigymaa"):FindFirstChild("stigre") then
			script.Parent.Parent["infected text"].Text = "True"
			script.Parent.Parent["infected text"].TextColor3 = Color3.new(0.384314, 1, 0)
		else
			script.Parent.Parent["infected text"].Text = "False"
			script.Parent.Parent["infected text"].TextColor3 = Color3.new(1, 0, 0)
		end
	end
end
coroutine.wrap(XASNBP_fake_script)()
