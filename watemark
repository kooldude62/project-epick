epick2.ZIndex = 399
epick2.Image = "rbxassetid://11687089808"

UIAspectRatioConstraint_2.Parent = epick2

-- Scripts:

local function JGYWCX_fake_script() -- Frame.LocalScript 
	local script = Instance.new('LocalScript', Frame)

	script.Parent.TextLabel.Text = "Project Epick, made by epick, welcome ".. game.Players.LocalPlayer.Name .." :D"
end
coroutine.wrap(JGYWCX_fake_script)()
local function SSKCHQF_fake_script() -- epickglasses.LocalScript 
	local script = Instance.new('LocalScript', epickglasses)

	gui_name = script.Parent
	
	while true do
		wait(0.01)
		gui_name.Rotation = gui_name.Rotation +3
	end
end
coroutine.wrap(SSKCHQF_fake_script)()
local function HDNQV_fake_script() -- epick2.LocalScript 
	local script = Instance.new('LocalScript', epick2)

	gui_name = script.Parent
	
	while true do
		wait(0.01)
		gui_name.Rotation = gui_name.Rotation +3
	end
end
coroutine.wrap(HDNQV_fake_script)()
local function ITHNDWA_fake_script() -- epick2.LocalScript 
	local script = Instance.new('LocalScript', epick2)

	script.Parent.MouseEnter:Connect(function()
	script.Parent.Parent.Frame.Visible = true
	sounds:Play()
	end)
	script.Parent.MouseLeave:Connect(function()
		script.Parent.Parent.Frame.Visible = false
	end)
	
end
coroutine.wrap(ITHNDWA_fake_script)()
