local function DECRJ_fake_script() -- ImageLabel.LocalScript 
	local script = Instance.new('LocalScript', ImageLabel)

	local TweenService = game:GetService("TweenService")
	local imageLabel = script.Parent
	
	local assetIds = {
		96174918368775,
		117555866533851,
		109587943866660,
		135809672464194,
		70617395524364,
		92145764665865,
		132190212161633,
		135715935095139,
		125757308820721,
		140114862066583,
		75429824027906
	}
	
	local prefix = "rbxassetid://"
	
	local tweenInfoFadeOut = TweenInfo.new(0.15, Enum.EasingStyle.Quad, Enum.EasingDirection.Out)
	local tweenInfoFadeIn = TweenInfo.new(0.15, Enum.EasingStyle.Quad, Enum.EasingDirection.In)
	
	local fadeOut = TweenService:Create(imageLabel, tweenInfoFadeOut, { ImageTransparency = 1 })
	local fadeIn = TweenService:Create(imageLabel, tweenInfoFadeIn, { ImageTransparency = 0 })
	
	while true do
		for _, id in ipairs(assetIds) do
			fadeOut:Play()
			fadeOut.Completed:Wait()
			imageLabel.Image = prefix .. id
			fadeIn:Play()
			fadeIn.Completed:Wait()
			wait(1.18)
		end
	end
	
end
coroutine.wrap(DECRJ_fake_script)()
local function DBJVE_fake_script() -- ImageLabel.Hover sc 
	local script = Instance.new('LocalScript', ImageLabel)

	local imageLabel = script.Parent
	local infoList = imageLabel:WaitForChild("Info list")
	
	infoList.Visible = false
	
	imageLabel.MouseEnter:Connect(function()
		infoList.Visible = true
	end)
	
	imageLabel.MouseLeave:Connect(function()
		infoList.Visible = false
	end)
end
coroutine.wrap(DBJVE_fake_script)()
local function MJJODV_fake_script() -- ImageLabel.LocalScript 
	local script = Instance.new('LocalScript', ImageLabel)

	while wait() do
		script.Parent.Rotation = math.sin(tick()*3)
	end
end
coroutine.wrap(MJJODV_fake_script)()
